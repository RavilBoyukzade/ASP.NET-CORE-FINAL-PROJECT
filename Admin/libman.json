{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.5.0",
      "destination": "wwwroot/lib/twitter-bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.min.css"
      ]
    },
    {
      "library": "Chart.js@2.9.3",
      "destination": "wwwroot/lib/Chart.js/"
    },
    {
      "library": "datatables@1.10.20",
      "destination": "wwwroot/lib/datatables/"
    },
    {
      "library": "font-awesome@5.13.0",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "jquery-easing@1.4.1",
      "destination": "wwwroot/lib/jquery-easing/"
    }
  ]
