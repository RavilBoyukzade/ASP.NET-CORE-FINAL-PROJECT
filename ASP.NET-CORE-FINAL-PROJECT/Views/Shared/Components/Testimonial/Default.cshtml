@model IEnumerable<TestimonialViewModel>

@foreach (var item in Model)
{
    <div class="testimonials-slider-item">
        <div class="grid">
            <p>@item.Text</p>
            <div class="quoter">
                <h4>@item.Name</h4>
                <span>@item.Position</span>
            </div>
        </div>
    </div>

}
